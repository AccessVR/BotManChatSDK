{
    "name": "orchestratexr/botman-chat-sdk",
    "version": "0.0.17",
    "description": "",
    "keywords": [
        "botman",
        "chat",
        "sdk",
        "php",
        "laravel",
        "llphant",
        "openai"
    ],
    "homepage": "https://github.com/AccessVR/BotManChatSDK",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Aaron Collegeman",
            "email": "aaroncollegeman@gmail.com"
        },
        {
            "name": "AccessVR",
            "email": "support@accessvr.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "botman/botman": "^2.8",
        "botman/driver-web": "^1.5",
        "illuminate/broadcasting": "^10.0|^11.0|^12.0",
        "illuminate/contracts": "^10.0|^11.0|^12.0",
        "illuminate/events": "^10.0|^11.0|^12.0",
        "illuminate/http": "^10.0|^11.0|^12.0",
        "illuminate/queue": "^10.0|^11.0|^12.0",
        "illuminate/support": "^10.0|^11.0|^12.0",
        "illuminate/view": "^10.0|^11.0|^12.0",
        "league/commonmark": "^2.6",
        "theodo-group/llphant": "^0.11.0"
    },
    "autoload": {
        "psr-4": {
            "OrchestrateXR\\BotManChatSDK\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "OrchestrateXR\\BotManChatSDK\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "php-http/discovery": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "OrchestrateXR\\BotManChatSDK\\BotManChatServiceProvider"
            ],
            "aliases": {
                "BotManChat": "OrchestrateXR\\BotManChatSDK\\BotManChat"
            }
        }
    }
}
